{
    "name": "libswscale",
    "version": "5.1.0",
    "frameworks": "*",
    "platforms": "*",
	"build": {
		"srcDir": "libswscale",
		"includeDir": ".",
		"srcFilter": [
			"-<*>"
		],
		"flags": [
			"-D__FREERTOS__",
			"-I libswscale",
			"-Wno-pointer-sign",
			"-Wno-parentheses",
			"-Wno-switch",
			"-Wno-maybe-uninitialized"
		]
	}
}
